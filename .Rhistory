#print(sum(adj_temps*(adj_temps > 0))/24)
mon_sum[nrow(mon_sum) + 1,] = c(y,m, sum(adj_temps*(adj_temps > 0))/24)
}
}
mon_sum$City = data$City[1]
return(mon_sum)
}
